<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="project.sd.client.controller.AdminController"
            prefHeight="300.0" prefWidth="280.0">

    <children>
        <Button fx:id="books" layoutX="90.0" layoutY="50.0" prefHeight="40.0" prefWidth="100.0" mnemonicParsing="false" onAction="#goBooks" text="Books" />
        <Button fx:id="users" layoutX="90.0" layoutY="130.0" prefHeight="40.0" prefWidth="100.0" mnemonicParsing="false" onAction="#goUsers" text="Users" />
        <Button fx:id="pdfReport" layoutX="30.0" layoutY="210.0" prefHeight="40.0" prefWidth="100.0" mnemonicParsing="false" onAction="#generatePdf" text="Generate PDF" />
        <Button fx:id="txtReport" layoutX="150.0" layoutY="210.0" prefHeight="40.0" prefWidth="100.0" mnemonicParsing="false" onAction="#generateTxt" text="Generate TXT" />
    </children>

</AnchorPane>
