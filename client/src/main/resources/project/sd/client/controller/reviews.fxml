<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="project.sd.client.controller.ReviewsController"
            prefHeight="450.0" prefWidth="960.0">

    <TableView fx:id="tableViewReviews" GridPane.columnIndex="0"
               layoutX="30"
               layoutY="20"
               prefWidth="905"
               GridPane.rowIndex="1">
        <columns>
            <TableColumn fx:id="title" text="Book's Title" prefWidth="200">
            </TableColumn>
            <TableColumn fx:id="author" text="Book's Author" prefWidth="150">
            </TableColumn>
            <TableColumn fx:id="reviewText" text="Review" prefWidth="250">
            </TableColumn>
            <TableColumn fx:id="firstName" text="First Name" prefWidth="150">
            </TableColumn>
            <TableColumn fx:id="lastName" text="Last Name" prefWidth="150">
            </TableColumn>
        </columns>
    </TableView>

</AnchorPane>
